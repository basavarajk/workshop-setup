{
  "name": "workshop-setup",
  "version": "0.0.0-development",
  "description": "Verify and setup a repository for workshop attendees",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "scripts": {
    "build": "webpack",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "files": [
    "dist",
    "run-setup.js"
  ],
  "bin": {
    "workshop-setup": "run-setup.js"
  },
  "keywords": [
    "workshop",
    "tool",
    "utility",
    "repository"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "indent-string": "^3.2.0",
    "jest-in-case": "^1.0.2",
    "kcd-scripts": "^1.2.0",
    "semver": "^6.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/kcd-scripts/eslint.js"
    ],
    "rules": {
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8.0.0"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/workshop-setup.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/workshop-setup/issues"
  },
  "homepage": "https://github.com/kentcdodds/workshop-setup#readme"
}
